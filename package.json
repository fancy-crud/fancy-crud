{
  "name": "fancy-crud",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/fancy-crud.umd.js",
  "module": "./dist/fancy-crud.es.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "dev": "vite --port 9090 --host 0.0.0.0 --open",
    "build": "node build/exports.js && vite build && tsc --emitDeclarationOnly",
    "semantic-release": "semantic-release --branches main"
  },
  "exports": {
    ".": {
      "import": "./dist/fancy-crud.es.js",
      "require": "./dist/fancy-crud.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "peerDependencies": {
    "@mdi/font": "^6.7.96",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "esno": "0.10.0",
    "flowbite": "^1.4.7",
    "flowbite-datepicker": "^1.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "vue": "^3.2.19",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@mdi/font": "^6.7.96",
    "@vueuse/core": "^9.9.0",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "esno": "0.10.0",
    "flowbite": "^1.4.7",
    "flowbite-datepicker": "^1.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "vue": "^3.2.19",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.10.2",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "fast-glob": "^3.2.7",
    "postcss": "^8.4.14",
    "sass": "^1.52.3",
    "semantic-release": "^19.0.3",
    "tailwindcss": "^3.1.4",
    "tsup": "^5.2.1",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.17.11",
    "vite": "^2.6.2",
    "vite-plugin-windicss": "^1.4.9"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cafadev/fancy-crud.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu",
    "ignorePatterns": [
      "build/**/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
